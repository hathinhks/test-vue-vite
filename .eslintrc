{
  "root": true,
  "env": {
    "node": true
  },
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@typescript-eslint/parser"
  },
  "plugins": ["@typescript-eslint"],
  "extends": [
    "plugin:vue/strongly-recommended",
    "eslint:recommended",
    "prettier"
  ],
  "rules": {
    "vue/no-unused-vars": "error"
  }
}
